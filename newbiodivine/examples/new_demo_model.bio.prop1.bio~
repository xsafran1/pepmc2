VARS:A,B,C

PARAMS: c, 0, 1; a, 0, 1

CONSTS: th_1,0.0001; th_2,4; k~,4.234235; S_a,3; S_b,0.5; H_a,0; H_b,2

EQ:A = (-c)*A*B*Sm(A,k~,th_1,S_b,S_a)*(Hp(B,th_2,H_a,H_b,1) - A)
EQ:B = (-c)*A*B*Hillp(A,th_1,0.8,0,1)
EQ:C = a*A*B



THRES:A: 0.0001, 6, 10
THRES:B: 0.0001, 4, 6
THRES:C: 0.0001, 2, 4, 6

INIT: A:6,10; B:4,6; C:0.0001,2

VAR_POINTS: B: 200, 10


process LTL_property {
state q1, q2;
init q1;
accept q2;
trans
q1 -> q2 { guard not B>4; },
q1 -> q1 {},
q2 -> q2 { guard not B>4; },
q2 -> q1 {};
}

system async property LTL_property;
