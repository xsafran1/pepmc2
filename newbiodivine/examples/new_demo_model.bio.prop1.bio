VARS:A,B,C

PARAMS: c, 0, 1; a, 0, 1

EQ:A = (-c)*A*B
EQ:B = (-c)*A*B
EQ:C = a*A*B

THRES:A: 0.0001, 6, 10
THRES:B: 0.0001, 4, 6
THRES:C: 0.0001, 2, 4, 6

INIT: A:6,10; B:4,6; C:0.0001,2


process LTL_property {
state q1, q2;
init q1;
accept q2;
trans
q1 -> q2 { guard not B>4; },
q1 -> q1 {},
q2 -> q2 { guard not B>4; },
q2 -> q1 {};
}

system async property LTL_property;
